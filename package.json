{
  "name": "postbird",
  "main": "main.js",
  "version": "0.8.4",
  "repository": "https://github.com/Paxa/postbird",
  "license": "MIT",
  "description": "Postbird is a cross-platform PostgreSQL GUI client, written in JavaScript, runs with Electron",
  "scripts": {
    "sass": "sass assets/styles/style.scss:public --watch",
    "test": "electron-mocha tests/ --renderer --require-main main.js",
    "integration_test": "mocha integration_tests/integration.js",
    "start": "cross-env NW_DEV=true NW_DEBUG=true electron . $@",
    "rebuild_ext": "cross-env PATH=$PATH:/Applications/Postgres.app/Contents/Versions/latest/bin electron-rebuild",
    "ts": "node ts.js",
    "dist": "node build_package.js",
    "build": "node build_package.js",
    "lint": "eslint ."
  },
  "author": {
    "name": "Pavel Evstigneev",
    "email": "pavel.evst@gmail.com",
    "url": "https://github.com/Paxa"
  },
  "devDependencies": {
    "@types/alertify": "^0.3.30",
    "@types/jquery": "^3.5.3",
    "@types/mousetrap": "^1.6.4",
    "@types/pg": "^7.14.5",
    "@types/semver": "~7.3.8",
    "electron": "^21.2.3",
    "electron-builder": "^23.6.0",
    "electron-mocha": "^13.0.0",
    "eslint": "^7.32.0",
    "localStorage": "^1.0.4",
    "plus-typescript": "^4.1.1-alpha",
    "sass": "^1.49.9",
    "spectron": "^19.0.0"
  },
  "dependencies": {
    "@electron/remote": "2.0.1",
    "@sentry/electron": "2.5.4",
    "@sentry/integrations": "6.14.0",
    "colors": "^1.4.0",
    "connection-string": "4.3.4",
    "cross-env": "^7.0.3",
    "csv-stringify": "^5.6.5",
    "electron-window-state": "5.0.3",
    "eventemitter2": "6.4.5",
    "is-electron-renderer": "^2.0.1",
    "moment": "^2.29.1",
    "needle": "2.8.0",
    "node-ssh-forward": "https://github.com/Paxa/node-ssh-forward.git#add-timeout",
    "pg": "8.16.3",
    "pg-escape": "^0.2.0",
    "pug": "3.0.2",
    "semver": "^7.3.5",
    "slash": "^3.0.0",
    "sprintf-js": "1.1.2",
    "ssh-config": "^4.1.0",
    "strftime": "0.10.0"
